{
  "expo": {
    "name": "IsDone Plus",
    "slug": "to-do-list",
    "scheme": "isdoneplus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#71D7E4"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.davidsliren.isdoneplus"
    },
    "android": {
      "package": "com.davidsliren.isdoneplus",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#71D7E4"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router", "expo-font"]
  }
}
